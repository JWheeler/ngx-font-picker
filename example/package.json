{
  "name": "angular2-font-picker-example",
  "license": "MIT",
  "version": "0.3.0",
  "tslint": {
    "extends": "./tslint.json"
  },
  "stylelint": {
    "extends": "./stylelint.json"
  },
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "build": "webpack --config config/webpack.config.js",
    "start": "webpack-dev-server --open --inline --config config/webpack.config.js",
    "deploy": "rm -rf dist >/dev/null 2>&1; deploy-to-git",
    "lint": "tslint 'src/**/*.ts' ; stylelint 'src/**/*.css' --syntax css"
  },
  "config": {
    "deployToGit": {
      "repository": "git@github.com:zefoy/angular2-font-picker.git",
      "branch": "gh-pages",
      "folder": "dist",
      "script": "npm run build",
      "commit": "Publishing $npm_package_version",
      "user": {
        "name": "ZEF Devel",
        "email": "devel@zef.fi"
      }
    }
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "angular2-font-picker": "0.3.0",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "webfontloader": "1.6.26",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.0",
    "@types/node": "^6.0.0",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.0",
    "copy-webpack-plugin": "^3.0.0",
    "deploy-to-git": "0.0.7",
    "html-webpack-plugin": "^2.22.0",
    "raw-loader": "^0.5.0",
    "stylelint": "7.2.0",
    "tslint": "3.15.0",
    "typescript": "^2.0.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.15.0"
  }
}
